DROP TABLE IF EXISTS `recurringExceptions`;

CREATE TABLE `recurringExceptions` (
  `id` char(11) NOT NULL,
  `churchId` char(11) DEFAULT NULL,
  `recurringEventId` char(11) DEFAULT NULL,
  `eventId` char(11) DEFAULT NULL,
  `originalDate` datetime DEFAULT NULL,
  `newDate` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
