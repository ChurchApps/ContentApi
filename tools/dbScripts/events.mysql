DROP TABLE IF EXISTS `events`;

CREATE TABLE `events` (
  `id` char(11) NOT NULL,
  `churchId` char(11) DEFAULT NULL,
  `groupId` char(11) DEFAULT NULL,
  `eventDate` datetime DEFAULT NULL,
  `title` varchar(255) DEFAULT NULL,
  `description` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
