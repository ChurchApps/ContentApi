DROP TABLE IF EXISTS `recurringEvents`;

CREATE TABLE `recurringEvents` (
  `id` char(11) NOT NULL,
  `churchId` char(11) DEFAULT NULL,
  `groupId` char(11) DEFAULT NULL,
  `startDate` datetime DEFAULT NULL,
  `rule` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
